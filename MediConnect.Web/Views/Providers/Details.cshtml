@model MediConnect.Web.Models.Provider
@{ ViewData["Title"] = "Provider Details"; }
<h2>@Model.FullName</h2>
<ul>
  <li><b>Specialty:</b> @Model.Specialty</li>
  <li><b>State:</b> @Model.State</li>
  <li><b>NPI Number:</b> @Model.NpiNumber</li>
</ul>
<p>
  <a asp-action="Edit" asp-route-id="@Model.Id">Edit</a> |
  <a asp-action="Index">Back</a>
</p>
